{
    "compilerOptions": {
        "outDir": "bin",
        "target": "ES2020",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "apiKey.ts",
        "config/index.ts",
        "config/vars.ts",
        "domain.ts",
        "domainCredential.ts",
        "getDomain.ts",
        "index.ts",
        "provider.ts",
        "route.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts",
        "webhook.ts"
    ]
}
